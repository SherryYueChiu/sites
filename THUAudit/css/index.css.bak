::-webkit-scrollbar {
    width: 0;
    height: 0;
}
html, body, #app {
  width: 100%;
  height: 100%;
  overflow-y: auto;
}
a:hover {
  color: #EF9A9A;
}
#app {
  background-color: #FFCCBC;
}
#page2 {
	display: none;
}
.page {
    overflow-y: auto;
}
.classCode {
	text-align: right;
    margin-top: -1.5em;
}
hr.style-one {
    height: 1px;
    background: #333;
    background-image: linear-gradient(to right, #333, #ccc, #333);
}
#footer {
  position: fixed;
  bottom: 10px;
  width: 100%;
  height: 10px;
  z-index: 99;
  opacity:0.6;
}

#footer>span {
  font-size: medium;
  margin-left: -2em;
}

.floatingBtn {
	position: fixed;
	width: 60px;
	height: 60px;
	bottom: 40px;
	right: 40px;
	background-color: #E8E9EB !important;
	color: #101412 !important;
	font-weight: bolder !important;
	border-radius: 50px;
	text-align: center;
	box-shadow: 2px 2px 3px #999;
	z-index: 99;
}

